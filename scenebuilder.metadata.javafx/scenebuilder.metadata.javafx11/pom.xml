<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 https://maven.apache.org/xsd/maven-4.0.0.xsd">
    <modelVersion>4.0.0</modelVersion>
    <parent>
        <groupId>com.gluonhq.scenebuilder</groupId>
        <artifactId>scenebuilder.metadata.javafx</artifactId>
        <version>17.0.0-SNAPSHOT</version>
    </parent>
    <artifactId>scenebuilder.metadata.javafx11</artifactId>

    <properties>
        <javafx.version>11</javafx.version>
    </properties>

    <dependencies>
        <dependency>
            <groupId>com.gluonhq.scenebuilder</groupId>
            <artifactId>scenebuilder.core.metadata</artifactId>
            <version>17.0.0-SNAPSHOT</version>
            <exclusions>
                <exclusion>
                    <groupId>org.openjfx</groupId>
                    <artifactId>javafx-fxml</artifactId>
                </exclusion>
                <exclusion>
                    <groupId>org.openjfx</groupId>
                    <artifactId>javafx-web</artifactId>
                </exclusion>
                <exclusion>
                    <groupId>org.openjfx</groupId>
                    <artifactId>javafx-swing</artifactId>
                </exclusion>
            </exclusions>
        </dependency>
        <dependency>
            <groupId>org.openjfx</groupId>
            <artifactId>javafx-fxml</artifactId>
            <version>${javafx.version}</version>
        </dependency>
        <dependency>
            <groupId>org.openjfx</groupId>
            <artifactId>javafx-web</artifactId>
            <version>${javafx.version}</version>
        </dependency>
        <dependency>
            <groupId>org.openjfx</groupId>
            <artifactId>javafx-swing</artifactId>
            <version>${javafx.version}</version>
        </dependency>
    </dependencies>
    <build>
        <plugins>
            <plugin>
                <groupId>com.gluonhq.scenebuilder</groupId>
                <artifactId>scenebuilder.metadata-maven-plugin</artifactId>
                <version>17.0.0-SNAPSHOT</version>
                <executions>
                    <execution>
                        <id>generate-class-sources</id>
                        <phase>generate-sources</phase>
                        <goals>
                            <goal>metadataSource</goal>
                        </goals>
                    </execution>
                </executions>
                <configuration>
                    <targetPackage>com.oracle.javafx.scenebuilder.metadata.javafx</targetPackage>
                    <executorClass>com.oracle.javafx.scenebuilder.metadata.template.scenebuilderx.ScenebuilderxExecutor</executorClass>
                    <extensionName>JavafxMetadataExtension</extensionName>
                    <moduleName>scenebuilder.metadata.javafx</moduleName>
                    <uuid>5cadadb4-8aba-44f1-b66d-78e988cd998c</uuid>
                    <failOnError>false</failOnError>
                    <javafxVersion>${javafx.version}</javafxVersion>
                </configuration>
                <dependencies>
                    <dependency>
                        <groupId>com.gluonhq.scenebuilder</groupId>
                        <artifactId>scenebuilder.metadata.javafxdata</artifactId>
                        <version>17.0.0-SNAPSHOT</version>
                    </dependency>
                    <dependency>
                        <groupId>com.gluonhq.scenebuilder</groupId>
                        <artifactId>scenebuilder.core.metadata</artifactId>
                        <version>17.0.0-SNAPSHOT</version>
                        <exclusions>
                            <exclusion>
                                <groupId>com.gluonhq.scenebuilder</groupId>
                                <artifactId>scenebuilder.starter</artifactId>
                            </exclusion>
                        </exclusions>
                    </dependency>
                    <dependency>
                        <groupId>org.openjfx</groupId>
                        <artifactId>javafx-fxml</artifactId>
                        <version>${javafx.version}</version>
                    </dependency>
                    <dependency>
                        <groupId>org.openjfx</groupId>
                        <artifactId>javafx-web</artifactId>
                        <version>${javafx.version}</version>
                    </dependency>
                    <dependency>
                        <groupId>org.openjfx</groupId>
                        <artifactId>javafx-swing</artifactId>
                        <version>${javafx.version}</version>
                    </dependency>
                </dependencies>
            </plugin>
        </plugins>
    </build>
</project>