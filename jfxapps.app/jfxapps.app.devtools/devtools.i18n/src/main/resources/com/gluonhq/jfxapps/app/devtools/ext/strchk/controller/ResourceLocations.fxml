<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>

<AnchorPane maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <HBox AnchorPane.bottomAnchor="10.0" AnchorPane.leftAnchor="10.0" AnchorPane.rightAnchor="10.0" AnchorPane.topAnchor="10.0">
         <children>
            <Label text="numberOfValues : " />
            <Label fx:id="numberOfValues" text="Label" />
            <Label text="Ok : ">
               <HBox.margin>
                  <Insets left="20.0" />
               </HBox.margin>
            </Label>
            <Label fx:id="numberOfOk" text="Label" />
            <Label text="Error : ">
               <HBox.margin>
                  <Insets left="20.0" />
               </HBox.margin>
            </Label>
            <Label fx:id="numberOfErrors" text="Label" />
            <Label text="Undefined : ">
               <HBox.margin>
                  <Insets left="20.0" />
               </HBox.margin>
            </Label>
            <Label fx:id="numberOfUndefined" text="Label" />
            <Label text="Search : ">
               <HBox.margin>
                  <Insets left="40.0" />
               </HBox.margin>
            </Label>
            <TextField fx:id="searchField" onKeyPressed="#keyPressed" prefHeight="25.0" prefWidth="287.0" HBox.hgrow="ALWAYS" />
            <Label text="PRESS ENTER=Search/Next" />
            <Label text="Match : ">
               <HBox.margin>
                  <Insets left="20.0" />
               </HBox.margin>
            </Label>
            <Label fx:id="numberOfMatch" text="Label" />
            <CheckBox fx:id="applyFilterCheckbox" mnemonicParsing="false" text="Apply Config filters">
               <HBox.margin>
                  <Insets left="80.0" />
               </HBox.margin>
            </CheckBox>
         </children>
      </HBox>
      <TableView fx:id="valuesTable" editable="true" prefHeight="735.0" prefWidth="1260.0" AnchorPane.bottomAnchor="55.0" AnchorPane.leftAnchor="10.0" AnchorPane.rightAnchor="10.0" AnchorPane.topAnchor="50.0">
        <columns>
          <TableColumn fx:id="projectColumn" text="Project" />
          <TableColumn fx:id="packageColumn" text="Package" />
            <TableColumn fx:id="fileColumn" text="File" />
            <TableColumn fx:id="valueColumn" text="Value" />
            <TableColumn fx:id="matchProjectColumn" text="Match Project" />
            <TableColumn fx:id="matchPackageColumn" text="Match Package" />
            <TableColumn fx:id="matchFileColumn" text="Match File" />
            <TableColumn fx:id="matchErrorColumn" text="Error" />
            <TableColumn fx:id="matchSolutionColumn" text="Proposed Solution" />
        </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
      <HBox alignment="CENTER" spacing="200.0" AnchorPane.bottomAnchor="10.0" AnchorPane.leftAnchor="10.0" AnchorPane.rightAnchor="10.0">
         <children>
            <Button mnemonicParsing="false" onAction="#cancel" text="Cancel" />
            <Button mnemonicParsing="false" onAction="#update" text="Update" />
            <Button mnemonicParsing="false" onAction="#copy" text="Copy" />
         </children>
      </HBox>
   </children>
</AnchorPane>
